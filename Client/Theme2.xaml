<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:local="clr-namespace:Client">
    <Color x:Key="Color1">#21094E</Color>
    <Color x:Key="Color2">#635283</Color>
    <Color x:Key="Color3">#4CA1A3</Color>
    <Color x:Key="Color4">#7fd48a</Color>
    <Color x:Key="Color5">Black</Color>
    <Color x:Key="Color6">#46c156</Color>

    <SolidColorBrush x:Key="Color1Bg" Color="#21094E"></SolidColorBrush>
    <SolidColorBrush x:Key="Color2Bg" Color="#635283"></SolidColorBrush>
    <SolidColorBrush x:Key="Color3Bg" Color="#4CA1A3"></SolidColorBrush>
    <SolidColorBrush x:Key="Color4Bg" Color="#7fd48a"></SolidColorBrush>
    <SolidColorBrush x:Key="Color5Bg" Color="Black"></SolidColorBrush>
    <SolidColorBrush x:Key="Color6Bg" Color="#46c156"></SolidColorBrush>
    <SolidColorBrush x:Key="Color7Bg" Color="White"></SolidColorBrush>
    
</ResourceDictionary>